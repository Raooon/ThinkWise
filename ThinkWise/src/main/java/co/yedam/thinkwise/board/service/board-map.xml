<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.thinkwise.board.service.BoardMapper">

	<select id="boardList" resultType="co.yedam.thinkwise.board.service.BoardVO">
		SELECT * FROM board
	</select>
	
	<select id="boardSelect" resultType="co.yedam.thinkwise.board.service.BoardVO">
		SELECT * FROM WHERE board_no=#{boardNo}
	</select>
	
	<insert id="boardInsert">
		INSERT INTO board VALUES(board_seq.nextval,#{id},sysdate,null,#{title},#{contents},#{image},0,0)
	</insert>
	
	<update id="boardUpdate">
	
	</update>
	
	<delete id="boardDelete">
	
	</delete>
	
	<update id="boardHitUpdate">
	
	</update>
	
	<update id="boardLikesUpdate">
	
	</update>
	
	
	
</mapper>